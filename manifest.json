{
  "manifest_version": 2,

  "name": "myHome",
  "description": "Customised New Tab with important links",
  "version": "1.0",
  "icons": {
    "48": "assets/icon.png"
  },

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html",
    "default_title": "Quicklinks"
  },
  
    "chrome_url_overrides" : {
    "newtab": "home.html"
  },
    
  "permissions": [
    "tabs",
    "http://www.thehindu.com/*",
    "http://www.popsci.com/"
  ]
}