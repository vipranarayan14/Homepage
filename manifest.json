{
  "manifest_version": 2,
  "name": "myHome",
  "description": "Customised New Tab with important links",
  "version": "1.0",
  "icons": {
    "48": "assets/images/icon.png"
  },

  "options_page": "home/options.html",

  "chrome_url_overrides": {
    "newtab": "home/index.html"
  },

  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}